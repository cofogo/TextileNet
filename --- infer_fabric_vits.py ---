import torch
from baselines.TextileNet-fabric.models import vits

# Load the model weights
model = vits.load_from_checkpoint('baselines/TextileNet-fabric/vits_ckpt.pth')

# Set the model to evaluation mode
model.eval()

# Example image (replace with your actual image loading code)
example_image = torch.randn(1, 3, 224, 224)  # Replace this with your image loading and preprocessing

# Run inference
with torch.no_grad():
    output = model(example_image)

print(output)
